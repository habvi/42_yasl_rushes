	0:=:2
	!

	8:=:0
	( 1 - )
	=
	( 0 == ? 0:9 : 1:9 )
	0:=:8
} loop_cnt

{
	"Error:invalid arguments number\n"
	print
	#
	exit

} invalid_arg

{
	( # 4 != ? invalid_arg : none)
	!
	!
} remove_unnecessry_arg

{
	//print_stack
	0:=:1
	!
	//0:][:2
	][
	0:=:8
	@loop_cnt
	//"loop end\n"
	//print
	//print_stack
	!
	//2:
	//print
} cp_word

{
	"Error:invalid separator\n"
	print
	#
	exit
} invalid_separator

{
	1:=:0
	strlen
	(1 == ? none : invalid_separator)
} check_separator

{
	!
	#
	0
	>
	? 1 : 0
} clear_stack

{
	(6:= 0 > ? "\n" : "")
	print
	6:!
	0:6
} print_cr

{
	6:!
	1:6
	2:=:0
	print
} print_str

{
	2:=:0
	=
	1:=:0
	( == ? print_cr : print_str)

	2:!
	2:#:0
	( 0 == ? none : print_separate)
} print_separate

{
	0:6
} init_flag

